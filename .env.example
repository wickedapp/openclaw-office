# OpenClaw Office Environment Variables
# Copy to .env.local and fill in your values

# Gateway connection (overrides config file)
OPENCLAW_GATEWAY_URL=ws://127.0.0.1:18789
OPENCLAW_GATEWAY_TOKEN=your-gateway-token-here

# Office customization
OFFICE_NAME=My AI Office
OFFICE_STYLE=cyberpunk

# Image generation (optional - Gemini via Nano Banana)
GEMINI_API_KEY=
GOOGLE_API_KEY=

# Position detection (optional - Claude Vision)
ANTHROPIC_API_KEY=

# Telegram notifications (optional)
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=
TELEGRAM_WEBHOOK_SECRET=
